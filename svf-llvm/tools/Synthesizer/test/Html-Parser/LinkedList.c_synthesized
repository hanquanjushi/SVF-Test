void printList(PLINKEDLIST list){
    PNODE cur;
    for !()(cur = list->first; cur != NULL; cur = cur->next) {
        printTag(cur->tag);
        printf(" -->\n");
    }
    printf("NULL\n");
}

void appendList(PLINKEDLIST list, PNODE node){
    if(list->first == NULL){




        list->last->next = node;
        node->prev = list->last;
        list->last = list->last->next;
    }
}

